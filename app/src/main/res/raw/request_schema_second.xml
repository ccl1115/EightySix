<api>
    <request>
        <id>get_chat_id</id>
        <name>获取chatId</name>
        <path>/im/get/chatId</path>
        <sign/>
        <params>
            <param>postId</param>
            <param>commentId</param>
        </params>
    </request>

    <request>
        <id>chat_send</id>
        <name>发私聊</name>
        <path>/commit/im/send/privateChat</path>
        <sign/>
        <params>
            <param>chatId</param>
            <param>msgType</param>
            <param>msg</param>
            <param>postId</param>
            <param>commentId</param>
        </params>
    </request>

    <request>
        <id>chat_report</id>
        <name>举报聊天会话</name>
        <path>/commit/im/report/chat</path>
        <sign/>
        <params>
            <param>chatId</param>
        </params>
    </request>

    <request>
        <id>chat_fav_add</id>
        <name>收藏聊天会话</name>
        <path>/commit/im/favorite/addChat</path>
        <sign/>
        <params>
            <param>chatId</param>
        </params>
    </request>

    <request>
        <id>chat_fav_del</id>
        <name>取消收藏聊天会话</name>
        <path>/commit/im/favorite/delChat</path>
        <sign/>
        <params>
            <param>chatId</param>
        </params>
    </request>

    <request>
        <id>chat_fav_list</id>
        <name>聊天会话收藏列表</name>
        <path>/im/favorite/chat</path>
        <sign/>
        <responseClz>ChatFavListResponse</responseClz>
    </request>

    <request>
        <id>chat_ol_list</id>
        <name>聊天会话在线列表</name>
        <path>/im/chat/onlineStatus</path>
        <sign/>
        <params>
            <param>chatId</param>
        </params>
        <responseClz>ChatOnlineListResponse</responseClz>
    </request>

    <request>
        <id>login</id>
        <name>登录接口</name>
        <path>/passport/login</path>
        <sign/>
        <params>
            <param>phone</param>
            <param>password</param>
            <param>valiCode</param>
        </params>
    </request>
</api>