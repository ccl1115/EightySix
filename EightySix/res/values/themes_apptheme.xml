<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
        <item name="android:windowActionBar">false</item>
        <item name="android:windowBackground">@android:color/white</item>
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="android:windowNoTitle">true</item>
        <item name="topBarStyle">@style/Widget.TopBar.AppTheme</item>
        <item name="android:buttonStyle">@style/Widget.Button.AppTheme</item>
        <item name="android:editTextStyle">@style/Widget.EditText.AppTheme</item>
        <item name="android:checkboxStyle">@style/Widget.CheckBox.AppTheme</item>
        <item name="android:radioButtonStyle">@style/Widget.RadioButton.AppTheme</item>

        <item name="alp_drawable_btn_code_lock_default_holo">@drawable/alp_btn_default_dot</item>
        <item name="alp_drawable_btn_code_lock_touched_holo">@drawable/alp_btn_touch_dot</item>
        <item name="alp_drawable_indicator_code_lock_point_area_default_holo">@drawable/alp_btn_default_dot</item>
        <item name="alp_color_pattern_path">@color/alp_pattern_path_dark</item>
        <item name="aosp_drawable_indicator_code_lock_point_area_normal">
            @drawable/aosp_indicator_code_lock_point_area_green_holo
        </item>
    </style>


</resources>